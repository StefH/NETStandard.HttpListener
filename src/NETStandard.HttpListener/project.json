            }
        },
        "uap10.0": {
            "dependencies": {
                "Microsoft.NETCore.UniversalWindowsPlatform": "5.2.2",
                "System.Collections": "4.3.0",
                "System.IO": "4.3.0",
                "System.Linq": "4.3.0",
                "System.Runtime.Extensions": "4.3.0",
