{
    "version": "1.0.0.0",
    "title": "NETStandard.HttpListener",
    "description": "HttpListener for .NET Core (NETStandard) and Universal Windows Platform (UWP)",
    "authors": [ "Robert Sundström", "Stef Heyenrath" ],
    "copyright": "Copyright © Robert Sundström 2016",
    "packOptions": {
        "owners": [ "Stef Heyenrath" ],
